#include <stdio.h>
// function pointer

void sum(int a, int b)
{
	int c = a + b;
	printf("sum = %d\n", c);
}

void subtract(int a, int b)
{
	int c = a - b;
	printf("subtract = %d\n", c);
}

int main(void)
{

	ptr = sum;
	ptr(12, 4);
		// push 4
		// push 12
		// call [ptr]
		// pop bx
		// pop bx

	sum(12, 4);
		// push 4
		// push 12
		// call addr_sum
		// pop bx
		// pop bx
	return 0;
}
